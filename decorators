#timer decorator(function execution time)
import time

def timer(func):
    #*args,**kwargs are the params of func
    def wrapper(*args,**kwargs):
        start = time.time()
        retrun_value = func(*args,**kwargs)
        print('Time:', round(time.time()-start,2),'sec(s)')
        return retrun_value
    return wrapper


@timer
def f1(t):
    time.sleep(t)


f1(3)
